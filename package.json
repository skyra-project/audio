{
	"name": "@skyra/audio",
	"version": "1.0.1",
	"description": "A TypeScript wrapper for Lavalink.",
	"author": "@skyra",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"require": "./dist/index.js",
		"import": "./index.mjs"
	},
	"homepage": "https://skyra-project.github.io/audio",
	"private": false,
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"scripts": {
		"prepublishOnly": "yarn build",
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write {src,tests}/**/*.ts",
		"docs": "typedoc",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive --latest",
		"clean": "tsc -b src --clean",
		"build": "tsc -b src",
		"postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"watch": "tsc -b src -w",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz"
	},
	"dependencies": {
		"backoff": "^2.5.0",
		"ws": "^7.4.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@sapphire/prettier-config": "^1.0.1",
		"@skyra/eslint-config": "^5.3.3",
		"@types/backoff": "^2.5.1",
		"@types/jest": "^26.0.19",
		"@types/node": "^14.14.14",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.11.0",
		"@typescript-eslint/parser": "^4.11.0",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.11.2",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.0",
		"gen-esm-wrapper": "^1.1.1",
		"husky": "^4.3.6",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"standard-version": "^9.0.0",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.1.1",
		"typedoc": "^0.19.2",
		"typedoc-plugin-nojekyll": "^1.0.1",
		"typescript": "^4.1.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/audio.git"
	},
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"keywords": [
		"audio",
		"lavalink",
		"typescript",
		"ts",
		"utility"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/audio/issues"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"acorn": "^8.0.4",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"dot-prop": "^6.0.1"
	},
	"prettier": "@sapphire/prettier-config"
}
